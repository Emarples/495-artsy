            "user_id": "1",
            "functions": ["create_user"],
            "args": [("1", {"flow": 10,"volume": 20}, 0)],
            "expected": {
                "user_id": "1",
                "coins": 0,
                "brushes": [],
                "paints": [],
                "baseline": {
                    "flow": 10,
                    "volume": 20
                },
                "history": [],
                "backgrounds": [],
                "drawings": [],
                "lastBreath": 0
            }
        },
        {
            "name": "get_user_attr not found",
            "description": "Checks that the get_user_attr function returns an empty dict if userId not found",
            "user_id": "2",
            "functions": [],
            "args": [],
            "expected": "None"
        },
        {
